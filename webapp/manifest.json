{
    "_version": "1.65.0",
    "sap.app": {
        "id": "com.invertions.sapfiorimodinv",
        "type": "application",
        "i18n": {
            "bundleName": "com.invertions.sapfiorimodinv.i18n.i18n",
            "fallbackLocale": "es_MX",
            "supportedLocales": [
                "es_MX"
            ]
        },
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.16.4",
            "toolsId": "994b0345-20cb-4584-8e8d-af85135e6071"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.132.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ui.unified": {},
                "sap.tnt": {},
                "sap.f": {},
                "sap.ui.table": {},
                "sap.ui.layout": {},
                "sap.viz": {},
                "sap.ui.comp": {},
                "sap.suite.ui.microchart": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
             "": { 
                "type": "sap.ui.model.json.JSONModel"
            },
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.invertions.sapfiorimodinv.i18n.i18n",
                    "fallbackLocale": "es_MX",
                    "supportedLocales": [
                        "es_MX"
                    ]
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.invertions.sapfiorimodinv.view",
                "controlId": "mainContents",
                "controlAggregation": "pages",
                "transition": "slide"
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "Login",
                    "target": "TargetLogin"
                },
                {
                    "pattern": "home",
                    "name": "RouteMain",
                    "target": "TargetMain"
                },
                {
                    "pattern": "products/{skuid}/select-presentation",
                    "name": "RouteSelectPresentationToEdit",
                    "target": "TargetSelectPresentationToEdit"
                },
                {
                    "pattern": "add-presentation/{skuid}",
                    "name": "RouteAddPresentation",
                    "target": "TargetAddPresentation"
                },
                {
                    "pattern": "products/{skuid}/presentations/{presentationId}/edit",
                    "name": "RouteEditPresentation",
                    "target": "TargetEditPresentation"
                },
                {
                    "pattern": "add-product",
                    "name": "RouteAddProduct",
                    "target": "TargetAddProduct"
                },
                {
                    "pattern": "salesforecast",
                    "name": "RouteSalesForecast",
                    "target": "TargetSalesForecast"
                },
                {
                    "pattern": "roles",
                    "name": "RouteRoles",
                    "target": "TargetRoles"
                },
                {
                    "pattern": "roles/detail/{roleId}",
                    "name": "RouteRolesDetail",
                    "target": "TargetRolesDetail"
                },

                 {
                    "pattern": "users",
                    "name": "RouteUsersList",
                    "target": "TargetUsersList"
                },
                {
                    "pattern": "users/{USERID}",
                    "name": "RouteUserDetails",
                    "target":"TargetUserDetails"
                },
                 {
                    "pattern": "catalogs",
                    "name": "RouteCatalogs",
                    "target": "TargetCatalogs"
                  },
                                    {
                                        "pattern": "investments",
                                        "name": "RouteInvestments",
                                        "target": "TargetInvestments"
                                    },
                                    {
                                        "pattern": "listasprecios",
                                        "name": "RouteListasPrecios",
                                        "target": "TargetListasPrecios"
                                    },
                                    {
                                        "pattern": "listasprecios/crear",
                                        "name": "RouteCrearLista",
                                        "target": "TargetCrearLista"
                                    },
                                    {
                                        "pattern": "listasprecios/{listaId}/editar",
                                        "name": "RouteEditarLista",
                                        "target": "TargetEditarLista"
                                    },
                                    {
                                        "pattern": "promociones",
                                        "name": "RoutePromociones",
                                        "target": "TargetPromociones"
                                    },
                                    {
                                        "pattern": "promociones/crear",
                                        "name": "RouteCrearPromocion",
                                        "target": "TargetCrearPromocion"
                                    },
                                    {
                                        "pattern": "promociones/calendario",
                                        "name": "RouteCalendario",
                                        "target": "TargetCalendario"
                                    },
                                    {
                                        "pattern": "categorias",
                                        "name": "RouteCategorias",
                                        "target": "TargetCategorias"
                                    },
                                    {
                                        "pattern": "config",
                                        "name": "RouteConfig",
                                        "target": "TargetConfig"
                                    }
            ],
            "targets": {
                "TargetMain": {
                    "viewName": "Main",
                    "viewId": "main",
                    "viewType": "XML"
                },
                "TargetAddProduct": {
                    "viewName": "AddProduct",
                    "viewId": "addProduct",
                    "viewType": "XML"
                },
                "TargetAddPresentation": {
                    "viewName": "AddPresentation",
                    "viewId": "addPresentation",
                    "viewType": "XML",
                    "viewPath": "com.invertions.sapfiorimodinv.view.presentaciones"
                },
                "TargetEditPresentation": {
                    "viewName": "EditPresentation",
                    "viewId": "editPresentation",
                    "viewType": "XML",
                    "viewPath": "com.invertions.sapfiorimodinv.view.presentaciones"
                },
                "TargetSelectPresentationToEdit": {
                    "viewName": "SelectPresentationToEditPage",
                    "viewId": "selectPresentationToEditPage",
                    "viewType": "XML",
                    "viewPath": "com.invertions.sapfiorimodinv.view.presentaciones"
                },
                "TargetSalesForecast": {
                    "viewName": "salesforecast.SalesForecast",
                    "viewId": "salesForecast",
                    "viewType": "XML",
                    "viewPath": "com.invertions.sapfiorimodinv.view.salesforecast"
                },
                "TargetRoles": {
                    "viewName": "Roles",
                    "viewId": "rolesSplitter",
                    "viewType": "XML",
                    "viewPath": "com.invertions.sapfiorimodinv.view.security.splitter"
                },
                "TargetUsersList": {
                    "viewId": "UsersList",
                    "viewName": "UsersList",
                    "viewType": "XML",
                    "viewPath": "com.invertions.sapfiorimodinv.view.security"
                },
                "TargetUserDetails":{
                    "viewId": "UserDetails",
                    "viewName": "UserDetails",
                    "viewType": "XML",
                    "viewPath": "com.invertions.sapfiorimodinv.view.security"
                },
                "TargetCatalogs": {
                    "viewName": "Catalogs",
                    "viewId": "catalogs",
                    "viewType": "XML",
                    "viewPath": "com.invertions.sapfiorimodinv.view.catalogs"
                },
                "TargetInvestments": {
                    "viewName": "Investments",
                    "viewId": "investmentsPage",
                    "viewType": "XML",
                    "viewPath": "com.invertions.sapfiorimodinv.view.investments"
                },
                "TargetLogin": {
                    "viewName": "Login",
                    "viewId": "login",
                    "viewType": "XML",
                    "viewPath": "com.invertions.sapfiorimodinv.view"
                },
                "TargetRolesDetail": {
                "viewName": "RolesDetail",
                "viewId": "rolesDetail",
                "viewType": "XML",
                "viewPath": "com.invertions.sapfiorimodinv.view.security"
                },
                "TargetListasPrecios": {
                        "viewName": "ListasPrecios",
                        "viewId": "listasPrecios",
                        "viewType": "XML",
                        "viewPath": "com.invertions.sapfiorimodinv.view.listasprecios"
                },
                "TargetCrearLista": {
                        "viewName": "CrearLista",
                        "viewId": "crearLista",
                        "viewType": "XML",
                        "viewPath": "com.invertions.sapfiorimodinv.view.listasprecios"
                },
                "TargetEditarLista": {
                        "viewName": "CrearLista",
                        "viewId": "editarLista",
                        "viewType": "XML",
                        "viewPath": "com.invertions.sapfiorimodinv.view.listasprecios"
                },
                "TargetPromociones": {
                        "viewName": "Promociones",
                        "viewId": "promociones",
                        "viewType": "XML",
                        "viewPath": "com.invertions.sapfiorimodinv.view.promociones"
                },
                "TargetCrearPromocion": {
                        "viewName": "CrearPromocion",
                        "viewId": "crearPromocion",
                        "viewType": "XML",
                        "viewPath": "com.invertions.sapfiorimodinv.view.promociones"
                },
                "TargetCalendario": {
                        "viewName": "Calendario",
                        "viewId": "calendario",
                        "viewType": "XML",
                        "viewPath": "com.invertions.sapfiorimodinv.view.promociones"
                },
                "TargetCategorias": {
                        "viewName": "Categorias",
                        "viewId": "categorias",
                        "viewType": "XML",
                        "viewPath": "com.invertions.sapfiorimodinv.view.categorias"
                },
                "TargetConfig": {
                        "viewName": "Config",
                        "viewId": "config",
                        "viewType": "XML",
                        "viewPath": "com.invertions.sapfiorimodinv.view"
                }
            }
        },
        "rootView": {
            "viewName": "com.invertions.sapfiorimodinv.view.App",
            "type": "XML",
            "id": "App"
        }
    }
}